{
  "mcpServers": {
    "render": {
      "command": "npx",
      "args": ["-y", "@anthropic-ai/mcp-server-render"],
      "env": {
        "RENDER_API_KEY": "${RENDER_API_KEY}"
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    }
  }
}
